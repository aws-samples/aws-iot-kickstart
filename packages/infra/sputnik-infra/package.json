    "clean": "tsc --build --clean && rm -rf lib",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:cdk": "tsc --project tsconfig.json",
    "build:files": "rsync -avm --exclude='*.ts' src/ lib",
    "prewatch": "yarn clean",
    "watch": "run-p watch:*",
    "watch:cdk": "yarn build:cdk --watch",
    "watch:files": "nodemon --watch src -e html,vtl,yml --exec yarn build:files",
    "test": "jest"
