{
    "private": true,
    "name": "@lambda/deployments-service",
    "description": "The deployments microservice for the sputnik",
    "main": "libs.js",
    "version": "0.0.6",
    "scripts": {
        "build": "mkdir -p ../../../../dist/@lambda && zip -FSrq ../../../../dist/$npm_package_name.zip *"
    },
    "devDependencies": {
        "@lambda/sputnik-lib": "^0.1.1",
        "aws-sdk": "*",
        "deep-diff": "1.0.2",
        "deepmerge": "2.2.1",
        "moment": "2.25.3",
        "underscore": "1.10.2",
        "uuid": "8.0.0"
    },
    "externals": [
        "aws-sdk"
    ],
    "workspaces": {
        "nohoist": [
            "moment",
            "moment/**",
            "underscore",
            "underscore/**",
            "deep-diff",
            "deep-diff/**",
            "deepmerge",
            "deepmerge/**",
            "uuid",
            "uuid/**"
        ]
    }
}
