{
    "private": true,
    "name": "@lambda/jit-onboarding-service",
    "description": "The Just In Time On Boarding microservice for sputnik",
    "main": "index.js",
    "version": "0.0.8",
    "scripts": {
        "build": "mkdir -p ../../../../dist/@lambda && zip -FSrq ../../../../dist/$npm_package_name.zip *"
    },
    "dependencies": {
        "@lambda/deployments-service": "^0.0.8",
        "@lambda/devices-service": "^0.0.5",
        "moment": "^2.27.0",
        "node-forge": "0.8.5",
        "shortid": "2.2.14",
        "underscore": "1.10.2"
    },
    "devDependencies": {
        "@lambda/sputnik-lib": "^0.1.3",
        "aws-sdk": "*"
    },
    "externals": [
        "aws-sdk"
    ],
    "//workspaces": {
        "nohoist": [
            "**/@lambda/devices-service",
            "**/@lambda/devices-service/**",
            "**/@lambda/deployments-service",
            "**/@lambda/deployments-service/**",
            "**/moment",
            "**/moment/**",
            "**/underscore",
            "**/underscore/**",
            "**/node-forge",
            "**/node-forge/**",
            "**/shortid",
            "**/shortid/**"
        ]
    }
}
