{
  "name": "@deathstar/sputnik-infra-lambda-code",
  "description": "Lambda functions used in infra",
  "version": "0.0.0-alpha.9",
  "license": "Apache-2.0",
  "author": "Jeremy Jonas <jay.jonas@hotmail.com>",
  "contributors": [
    "Timothee Cruse <timothee.cruse@gmail.com>",
    "Jeremy Jonas <jay.jonas@hotmail.com>",
    "Greg Varga <contact@gergelyvarga.com>",
    "Mirash Gjolaj <mirash.gjolaj@gmail.com>"
  ],
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --project tsconfig.json",
    "build:lambdas": "cd ../../../ && lerna run --stream 'build' --scope @lambda/*",
    "watch": "yarn build --watch",
    "test": "jest"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "1.51.0",
    "@aws-cdk/core": "1.51.0",
    "@deathstar/sputnik-core": "^0.0.0-alpha.3",
    "@deathstar/sputnik-infra-core": "^0.0.0-alpha.2"
  },
  "resolutions": {
    "deep-diff": "1.0.2",
    "deepmerge": "2.2.1",
    "moment": "2.25.3",
    "underscore": "1.10.2",
    "shortid": "2.2.13",
    "uuid": "8.0.0"
  },
  "workspaces": {
    "nohoist": [
      "deep-diff",
      "deep-diff/**",
      "deepmerge",
      "deepmerge/**",
      "moment",
      "moment/**",
      "underscore",
      "underscore/**",
      "shortid",
      "shortid/**",
      "uuid",
      "uuid/**"
    ]
  },
  "devDependencies": {
    "aws-sdk-mock": "^5.1.0"
  }
}
