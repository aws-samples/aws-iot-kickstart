{
  "$schema": "../../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MyThingsManagementWebsite": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.json",
            "assets": [
                "src/favicon.ico",
                "src/assets",
                { "glob": "**/*", "input": "./.local", "output": "/" },
                {
                    "glob": "underscore-min.js",
                    "input": "../../../node_modules/underscore",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "jquery.min.js",
                    "input": "../../../node_modules/jquery/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "jquery.slimscroll.min.js",
                    "input": "../../../node_modules/jquery-slimscroll",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "jquery.blockUI.js",
                    "input": "../../../node_modules/jquery-blockui",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "popper.min.js",
                    "input": "../../../node_modules/popper.js/dist/umd",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "bootstrap.min.js",
                    "input": "../../../node_modules/bootstrap/dist/js",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "bootstrap.min.css",
                    "input": "../../../node_modules/bootstrap/dist/css",
                    "output": "./assets/css"
                },
                {
                    "glob": "waves.min.js",
                    "input": "../../../node_modules/node-waves/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "sticky-kit.min.js",
                    "input": "../../../node_modules/sticky-kit/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "gauge.min.js",
                    "input": "../../../node_modules/gaugeJS/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "Chart.min.js",
                    "input": "../../../node_modules/chart.js/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "chartjs-plugin-annotation.min.js",
                    "input": "../../../node_modules/chartjs-plugin-annotation",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "chartjs-plugin-draggable.min.js",
                    "input": "../../../node_modules/chartjs-plugin-draggable/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "owl.carousel.min.js",
                    "input": "../../../node_modules/owl.carousel/dist",
                    "output": "./assets/ui-js"
                },
                {
                    "glob": "owl.carousel.min.css",
                    "input": "../../../node_modules/owl.carousel/dist/assets",
                    "output": "./assets/css"
                },
                {
                    "glob": "font-awesome.min.css",
                    "input": "../../../node_modules/font-awesome/css",
                    "output": "./assets/css"
                },
                {
                    "glob": "*.*",
                    "input": "../../../node_modules/font-awesome/fonts",
                    "output": "./assets/fonts"
                },
                {
                    "glob": "materialdesignicons.min.css",
                    "input": "../../../node_modules/@mdi/font/css",
                    "output": "./assets/css"
                },
                {
                    "glob": "*.*",
                    "input": "../../../node_modules/@mdi/font/fonts",
                    "output": "./assets/fonts"
                }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
                "../../../node_modules/systemjs/dist/system.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "MyThingsManagementWebsite:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "MyThingsManagementWebsite:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MyThingsManagementWebsite:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "MyThingsManagementWebsite-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MyThingsManagementWebsite:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MyThingsManagementWebsite:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "MyThingsManagementWebsite",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  }
}
