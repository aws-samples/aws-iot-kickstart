{
  "compilerOptions": {
    // "traceResolution": true,
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "../../",
    "baseUrl": ".",
    "lib": ["dom", "esnext"],
    "typeRoots": [
      "../../node_modules/@types",
      "../../@types",
    ]
  },
  "include": ["jest-dom/extend-expect"]
}
